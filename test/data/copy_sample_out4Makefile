all: chmod import_runqc import_read_counts copy_Exome_Diagnostik_diagnostic copy_SomaticAndTreatment_diagnostic queue_Exome_Diagnostik_diagnostic queue_SomaticAndTreatment_diagnostic

chmod:
	chmod -R 775 Analysis

import_runqc:
	php /mnt/storage2/megSAP/pipeline/src/NGS/runqc_parser.php -name "#00123" -run_dir Analysis/../ -force

import_read_counts:
	php /mnt/storage2/megSAP/pipeline/src/NGS/import_sample_read_counts.php -stats Analysis/Stats/Stats.json -db NGSD_TEST 

