queue_SomaticAndTreatment_diagnostic:
	php /mnt/storage2/megSAP/pipeline/src/NGS/db_queue_analysis.php -type 'single sample' -samples RX123456_03
	php /mnt/storage2/megSAP/pipeline/src/NGS/db_queue_analysis.php -type 'single sample' -samples DX181277_05 -args '-steps vc,cn,sv,re,db -use_dragen -no_abra'
